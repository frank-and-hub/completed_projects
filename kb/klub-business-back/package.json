{
  "name": "klub-business-back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev-start": "sleep 1 && nest start --watch",
    "start:dev-reset": "sleep 1 && npm run db:reset --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:reset": "npx prisma migrate reset --force && sleep 1 && npx prisma db seed",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:run": "npm run start:reset",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --watch --no-cache --config ./test/jest-e2e.json",
    "postinstall": "npx prisma generate",
    "db:start": "npm run prisma:reset && sleep 1 && npm run prisma:generate && sleep 1 && npx ts-node prisma/seed.ts && sleep 1 && npm run prisma:studio",
    "db:reset": "npm run prisma:reset && sleep 1 && npm run prisma:generate && sleep 1 && npx ts-node prisma/seed.ts && sleep 1 && npm run prisma:studio",
    "prisma": {
      "seed": "ts-node prisma/seed.ts"
    }
  },
  "dependencies": {
    "@as-integrations/express5": "^1.1.2",
    "@as-integrations/fastify": "^3.1.0",
    "@bull-board/express": "^6.14.0",
    "@keyv/redis": "^5.1.3",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/microservices": "^11.1.7",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-fastify": "^11.1.7",
    "@nestjs/platform-socket.io": "^11.1.7",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.7",
    "@prisma/client": "^6.19.0",
    "@types/passport-jwt": "^4.0.1",
    "agora-access-token": "^2.0.4",
    "apollo-server-express": "^3.13.0",
    "argon2": "^0.44.0",
    "axios": "^1.11.0",
    "bull": "^4.16.5",
    "bullmq": "^5.61.2",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "nest-winston": "^1.10.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "pactum": "^3.8.0",
    "passport": "^0.7.0",
